{
  "name": "snipping-wand",
  "productName": "Snipping Wand",
  "description": "test",
  "version": "0.0.0",
  "private": true,
  "author": "emranio <emranio@yahoo.com>",
  "copyright": "Â© 2022, emranio.",
  "homepage": "http://emran.io/snippingwand",
  "main": "dist/main.js",
  "build": {
    "appId": "io.emran.snippingwand",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test",
    "unit": "electron-mocha temp/specs.js --renderer --color --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test && webpack --config=build/webpack.e2e.config.js --env=test",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.6",
    "css-loader": "^6.7.1",
    "electron-builder": "^23.6.0",
    "electron-mocha": "^11.0.2",
    "mocha": "^10.0.0",
    "source-map-support": "^0.5.21",
    "spectron": "^19.0.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@mediapipe/selfie_segmentation": "^0.1.1675465747",
    "@tensorflow-models/body-segmentation": "^1.0.1",
    "@tensorflow/tfjs-backend-webgl": "^4.3.0",
    "@tensorflow/tfjs-core": "^4.3.0",
    "electron": "^23.1.4",
    "fs-jetpack": "^4.3.1"
  }
}
